#!/usr/bin/env sh
. "$(dirname -- "$0")/_/husky.sh"

echo '游끹游끹游끹游끹... Before push, Trying to build now. 游끹游끹游끹游끹'

echo '游 TypeScript check...'
pnpm types:check ||
(
  echo '游뱚 TypeScript errors detected!'
  false
)

pnpm build ||
(
    echo '游땸游땸游땸 Build failed. 游땸游땸游땸
            Please fix the build issues and try pushing again.'
    false;
)

echo '九九九九 You win this time... I am pushing this now. 九九九九' 
